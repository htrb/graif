      <menuitem name="ViewShowGraph" action="ViewShowGraphAction"/>
      <separator/>
      <menuitem name="ViewGoto" action="ViewGotoAction"/>
    </menu>
    <menu name="SettingMenu" action="SettingMenuAction">
      <menuitem name="SettingPreference" action="SettingPreferenceAction"/>
      <menuitem name="SettingBudget" action="SettingBudgetAction"/>
    </menu>
    <menu name="HelpMenu" action="HelpMenuAction">
      <menuitem name="HelpAbout" action="HelpAboutAction"/>
    </menu>
  </menubar>
  <toolbar name="Toolbar">
    <toolitem name="Save" action="FileSaveAction"/>

    <separator/>
    <toolitem name="ViewShowAccountSummary" action="ViewShowAccountSummaryAction"/>
    <toolitem name="ViewShowCategorySummary" action="ViewShowCategorySummaryAction"/>
    <toolitem name="ViewShowAccountInOut" action="ViewShowAccountInOutAction"/>
    <toolitem name="ViewShowItemSummary" action="ViewShowItemSummaryAction"/>
    <toolitem name="ViewShowMonthSummery" action="ViewShowMonthSummeryAction"/>
