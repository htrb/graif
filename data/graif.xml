    <toolitem name="PrevDay" action="ViewPrevDayAction"/>
    <toolitem name="Today" action="ViewTodayAction"/>
    <toolitem name="NextDay" action="ViewNextDayAction"/>
    <toolitem name="NextMonth" action="ViewNextMonthAction"/>
    <separator/>
    <toolitem name="GoBack" action="ViewGoBackAction"/>
    <toolitem name="GoForward" action="ViewGoForwardAction"/>
  </toolbar>
  <popup name="Popup" action="PopupAction">
    <menuitem name="EditCut" action="EditCutAction"/>
    <menuitem name="EditCopy" action="EditCopyAction"/>
    <menuitem name="EditPaste" action="EditPasteAction"/>
    <menuitem name="EditDelete" action="EditDeleteAction"/>
    <separator/>
  </popup>
</ui>
